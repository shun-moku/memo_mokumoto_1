<div class="contents row">
  <div class="container">
    <%= form_with(model: @note, local: true) do |form| %>
      <p class="new_url"><%= form.text_field :url, placeholder: "Url" %></p>
      <p class="new_memo"><%= form.text_area :memo, placeholder: "memo", rows: "10" %></p>
      <p><%= form.submit "SEND" %></p>
    <% end %>
  </div>
</div>